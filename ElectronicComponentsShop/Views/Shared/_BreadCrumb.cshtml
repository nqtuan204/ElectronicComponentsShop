@{
    Dictionary<string, string> BCTree = ViewBag.BCTree;
}

<!-- BREADCRUMB -->
<div id="breadcrumb" class="section">
    <!-- container -->
    <div class="container">
        <!-- row -->
        <div class="row">
            <div class="col-md-12">
                <ul class="breadcrumb-tree">
                    @foreach (var Key in BCTree.Keys)
                    {
                        <li><a href="@BCTree[Key]">@Key</a></li>
                    }
                </ul>
            </div>
        </div>
        <!-- /row -->
    </div>
    <!-- /container -->
</div>
<!-- /BREADCRUMB -->